{%{% extends 'base.html.twig' %}%}

{% block title %}Register{% endblock %}

{% block body %}
    <h1>Register</h1>

    {{ form_start(form) }}
        {{ form_row(form.username) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_row(form.plainPassword) }}
        <button type="submit" class="btn">Register</button>
    {{ form_end(form) }}
{% endblock %}